{
  "name": "discord-image-utils",
  "version": "0.0.6",
  "description": "Generate and modify images for use with Discord.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && mkdir -p dist/assets && cp -r src/assets/* dist/assets/",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "release": "changeset",
    "version": "changeset version",
    "publish": "pnpm run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.22.0",
    "@types/gifencoder": "^2.0.3",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "dependencies": {
    "canvas": "^3.1.0",
    "gifencoder": "^2.0.1",
    "jimp": "^1.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "discord",
    "discord.js",
    "image",
    "generation",
    "canvas",
    "meme",
    "filters",
    "effects",
    "manipulation",
    "gif",
    "animation"
  ],
  "author": "norysight",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GrishMahat/discord-image"
  },
  "bugs": {
    "url": "https://github.com/GrishMahat/discord-image/issues"
  },
  "homepage": "https://github.com/GrishMahat/discord-image#readme",
  "pnpm": {
    "onlyBuiltDependencies": [
      "canvas"
    ]
  }
}
